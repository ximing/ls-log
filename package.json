{
  "name": "ls-log",
  "version": "2.0.0",
  "description": "logger for wechat mmp or browser",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "i": "SASS_BINARY_SITE=https://npm.taobao.org/mirrors/node-sass/ npm install --registry=https://registry.npm.taobao.org",
    "prepublish": "npm run build",
    "build": "rimraf dist && mkdir dist && rsync -va -progress  ./src/* ./dist && babel --presets=es2015,stage-0,react  --plugins=transform-decorators-legacy src --out-dir dist",
    "test-watch": "export NODE_ENV=test && ava --watch",
    "test": "export NODE_ENV=test && nyc ava"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.3",
    "env-cmd": "^7.0.0",
    "eslint": "^4.16.0",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "date-format": "1.2.0",
    "object-inspect": "1.6.0"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "src/**/*.{js,jsx}",
      "test/*.js",
      "!dist/**/*",
      "!node_modules/**/*"
    ],
    "match": [
      "*",
      "!foo"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
